# Google Maps API Key (required)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# CRM API Configuration
# Option 1: Use local API endpoint (default, no config needed)
# The app will POST to /api/leads which logs to console

# Option 2: Forward leads to external CRM webhook (server-side)
# CRM_WEBHOOK_URL=https://your-crm.com/webhook/leads

# Option 3: Override API URL from client-side (legacy)
# NEXT_PUBLIC_CRM_API_URL=https://your-crm.com/api/leads
